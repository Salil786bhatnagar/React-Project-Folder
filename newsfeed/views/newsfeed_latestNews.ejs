  <section id="container" >
      <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
      <!--header start-->
     <%-include('newsfeed_headerAdminpainal')%>
      <!--header end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
      <!--sidebar start-->
       <%-include('newsfeed_sidebarAdminpainal')%>
      <!--sidebar end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
          	<h3><i class="fa fa-angle-right"></i>Latest Nesw</h3>
          	
          	<!-- BASIC FORM ELELEMNTS -->
          	<div class="row mt">
          		<div class="col-lg-12">
                  <div class="form-panel">
                        <h4 class="mb"><i class="fa fa-angle-right"></i>Information</h4>
                        <center><p style="color: darkcyan;"><b><%=msg%></b></p></center>
                      <form class="form-horizontal style-form" method="post" action="/admin_data/admin_insertlatestNesw" enctype="multipart/form-data">
                          <div class="form-group">
                              <label class="col-sm-2 col-sm-2 control-label">Title</label>
                              <div class="col-sm-10">
                                  <input type="text" name="title" placeholder="Enter you title" class="form-control">
                              </div>
                          </div>
                          <div class="form-group">
                              <label class="col-sm-2 col-sm-2 control-label">Discraiption</label>
                              <div class="col-sm-10">
                                <textarea name="discraiption" rows="4" cols="60" class="form-control"></textarea>
                              </div>
                          </div>
                          <div class="form-group">
                              <label class="col-sm-2 col-sm-2 control-label">Date</label>
                              <div class="col-sm-10">
                                  <input type="date" name="date" class="form-control">
                              </div>
                          </div>
                          <div class="form-group">
                              <label class="col-sm-2 col-sm-2 control-label">Image</label>
                              <div class="col-sm-10">
                                  <input name="image" type="file" class="form-control">
                              </div>
                          </div>
                          <div class="form-group">
                            <div class="col-sm-10">
                                <button type="submit" class="btn btn-theme">Submit</button>
                            </div>
                        </div>
                          
                      </form>
                  </div>
          		</div><!-- col-lg-12-->      	
          	</div><!-- /row -->
        </section>
      </section><!-- /MAIN CONTENT -->
      <!--main content end-->
      <!--footer start-->
      <!--footer end-->
  </section>
