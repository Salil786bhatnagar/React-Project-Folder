<!-- Header start  -->
<%-include('newsfeed_loginHeader')%>
<!-- Header end -->
	<div id="login-page">
	  	<div class="container">
				<form class="form-login" action="/admin_data/admin_checkLogin" method="post">
						<center><p style="color:red;background-color:black;" id="p_error"><b><%=msg%></b></p></center>
						<h2 class="form-login-heading">Admin sign in</h2>
						<div class="login-wrap">
							<input type="email" name="email" class="form-control" placeholder="Enter your email" autofocus>
							<br>
							<input type="password" name="password" class="form-control" placeholder="Enter your password">
							<label class="checkbox">
							</label>
							<button class="btn btn-theme btn-block" href="index.html" type="submit"><i class="fa fa-lock"></i> SIGN IN</button>
							<hr>
							
							<div class="login-social-link centered">
							</div>
							<div class="registration">
							</div>
						</div>
				</form>	  	
				<script type="text/javascript" src="/assets2/js/jquery.backstretch.min.js"></script>
				<script>
					$.backstretch("/assets2/img/login-bg.jpg", {speed: 500});
				</script>
	
	  	</div>
	</div>





