            <li><a href="/newsfeed_data/technology_News">Technology</a></li>
            <li><a href="/newsfeed_data/politics_News">Politics Post</a></li>
          </ul>
        </div>
    </div>
    </div>
  </section>
 <!-- footer start -->
 <%-include('newsfeed_footer')%>
 <!-- footer end -->
</div>
</body>
</html>