  <section id="container" >
      <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
      <!--header start-->
      <%-include('newsfeed_headerAdminpainal')%>
      <!--header end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
      <!--sidebar start-->
      <%-include('newsfeed_sidebarAdminpainal')%>
      <!--sidebar end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
          	<h3><i class="fa fa-angle-right"></i>User Data</h3>
              <div class="row mt">
                  <div class="col-md-12  col-sm-12 col-xl-12 col-xs-12">
                      <div class="content-panel">
                          <table class="table table-striped table-advance table-hover">
	                  	  	  <h4><i class="fa fa-angle-right"></i>User Display Data</h4>
	                  	  	  <hr>
                              <thead>
                              <tr>
                                  <th>Name</th>
                                  <th>Last Name</th>
                                  <th>Gender</th>
                                  <th>Address</th>
                                  <th>Contact</th>
                                  <th>Zip</th>
                                  <th>Place</th>
                                  <th>Email</th>
                                  <th>Picture</th>
                                  <!-- <th>Status</th> -->
                                  <th>Action</th>
                              </tr>
                              </thead>
                              <tbody>
                           <% data.map(function(item,index){%>       
                               <tr>
                                  <td><%=item.first_name%></td>
                                  <td><%=item.last_name%></td>
                                  <td><%=item.gender%></td>
                                  <td><%=item.address%></td>
                                  <td><%=item.contact%></td>
                                  <td><%=item.zip_code%></td>
                                  <td><%=item.place%></td>
                                  <td><%=item.email%></td>
                                  <td><img class="img-circle" src="/uploads/<%=item.picture%>" width="35px" height="35px" align=""></td>
                                  <!-- <td>status</td> -->
                                  <td>
                                      <span class="ved">
                                      <a href="/admin_data/userview_Data?u_id=<%=item.user_id%>">
                                        <button class="btn btn-success btn-xs">
                                          <i class="fa fa-check"></i>
                                        </button>
                                      </a>
                                      <a href="/admin_data/userselect_Data?edit_id=<%=item.user_id%>">
                                        <button class="btn btn-primary btn-xs">
                                          <i class="fa fa-pencil"></i>
                                        </button>
                                      </a>
                                      <a href="/admin_data/userselect_Data?edit_id=<%=item.user_id%>">
                                       <button class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>
                                      </a>
                                    </span>
                                  </td>
                              </tr>
                            <%
                            });
                            %>  
                              </tbody>
                          </table>
                      </div><!-- /content-panel -->
                  </div><!-- /col-md-12 -->
              </div><!-- /row -->

		</section><! --/wrapper -->
      </section><!-- /MAIN CONTENT -->

      <!--main content end-->
      <!--footer start-->
      
      <!--footer end-->
  </section>

